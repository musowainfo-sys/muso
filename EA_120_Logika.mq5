// EA_120_Logika.mq5\n\n// Expert Advisor for MetaTrader 5\n// Author: Your Name\n// Date: 2026-02-10\n\ninput double TakeProfit = 50; // Take Profit in points\ninput double StopLoss = 50;   // Stop Loss in points\ninput double LotSize = 0.1;    // Trading lot size\n\n//+------------------------------------------------------------------+\n//| Expert initialization function                                   |\n//+------------------------------------------------------------------+\nint OnInit()\n{\n    Print("EA_120_Logika initialized");\n    return(INIT_SUCCEEDED);\n}\n\n//+------------------------------------------------------------------+\n//| Expert deinitialization function                                 |\n//+------------------------------------------------------------------+\nvoid OnDeinit(const int reason)\n{\n    Print("EA_120_Logika deinitialized");\n}\n\n//+------------------------------------------------------------------+\n//| Expert tick function                                             |\n//+------------------------------------------------------------------+\nvoid OnTick()\n{\n    // Check for open orders\n    if (OrdersTotal() == 0) {\n        double AskPrice = NormalizeDouble(SymbolInfoDouble(_Symbol, SYMBOL_BID), _Digits);\n        double SellPrice = NormalizeDouble(SymbolInfoDouble(_Symbol, SYMBOL_ASK), _Digits);\n        \n        // Example trading logic - Buy\n        if (/* Your Buy Condition */)\n        {\n            double lot = LotSize;\n            double sl = AskPrice - StopLoss * _Point;\n            double tp = AskPrice + TakeProfit * _Point;\n            OrderSend(_Symbol, OP_BUY, lot, AskPrice, 2, sl, tp, "Buy Order", 0, 0, clrGreen);\n        }\n\n        // Example trading logic - Sell\n        if (/* Your Sell Condition */)\n        {\n            double lot = LotSize;\n            double sl = SellPrice + StopLoss * _Point;\n            double tp = SellPrice - TakeProfit * _Point;\n            OrderSend(_Symbol, OP_SELL, lot, SellPrice, 2, sl, tp, "Sell Order", 0, 0, clrRed);\n        }\n    }\n}\n//+------------------------------------------------------------------+\n